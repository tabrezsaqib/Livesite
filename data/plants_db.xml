  <data>
    <title>Acer Saccharum</title>
    <family>Aceraceae</family>
    <genus>Acer</genus>
    <species>saccharum</species>
    <common_name>Sugar maple</common_name>
    <french_common_name></french_common_name>
    <plant_type>woody</plant_type>
    <habitat>
